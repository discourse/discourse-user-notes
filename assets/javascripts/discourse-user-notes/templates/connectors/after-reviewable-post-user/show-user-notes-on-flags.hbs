{{#if userNotesCount}}
  <DButton
    @translatedTitle={{userNotesTitle}}
    @action={{action "showUserNotes"}}
    class="btn btn-flat"
  >
    {{#if emojiEnabled}}
      <img
        src={{emojiUrl}}
        title={{userNotesTitle}}
        alt="pencil"
        class="emoji"
      />
    {{else}}
      {{d-icon "sticky-note"}}
    {{/if}}
  </DButton>
{{/if}}